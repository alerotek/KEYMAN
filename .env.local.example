# Supabase Configuration
# Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Server-only Supabase Service Role Key (for admin operations)
# Keep this secret and never expose to client-side code
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Next.js Configuration
NEXT_PUBLIC_URL=http://localhost:3000,yourdomain.com

# Django Settings (if needed)
SECRET_KEY=your-django-secret-key-here
DEBUG=False
ALLOWED_HOSTS=localhost,127.0.0.1,yourdomain.com

# Database (if using Django DB instead of Supabase)
# DATABASE_URL=sqlite:///db.sqlite3
